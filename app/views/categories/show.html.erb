<% @category.catalogs.each do |x| %>
  <% if x.approved? %>
    <div class="card">
      <h5 class="card-header"><%= link_to x.title, catalog_path(x)%></h5>
      <div class="card-body">
        <% x.photos.each do |y| %>
          <%= image_tag(y.url, size: "300x200") %>
        <% end %>
      </div>
    </div>
  <% end %>
<% end %>
