Do not approved institution: <strong> <%= @catalogs.count %></strong>
<% @catalogs.each do |x| %>
  <div class="card">
    <h5 class="card-header"><%= link_to x.title, edit_catalog_path(x)%></h5>
    <div class="card-body">
      <% x.photos.each do |y| %>
        <%= image_tag(y.url, size: "300x200") %>
      <% end %>
    </div>
  </div>
<% end %>